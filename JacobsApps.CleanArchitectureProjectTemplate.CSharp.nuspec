<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>JacobsApps.CleanArchitectureProjectTemplate.CSharp</id>
    <version>1.3.3</version>
    <description>
      Creates a "clean architecture" project setup for a MVC / WebApi project.
    </description>
    <releaseNotes>Added a solition to quickly add pagination. Added a generic cached repository decorator for memory cache. Also added more unit tests to sample project.</releaseNotes>
    <authors>Jacob Duijzer</authors>
    <projectUrl>https://github.com/jacobduijzer/CleanArchitectureTemplate</projectUrl>
    <licenseUrl>https://github.com/jacobduijzer/CleanArchitectureTemplate/blob/master/LICENSE</licenseUrl>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
    <tags>csharp cleanarchitecture template project</tags>
  </metadata>
  <files>
    <file src=".\src\**" target="content" exclude="appveyor.yml;*.nuspec;**\bin\**;**\obj\**;**\.vs\**;**\*.user;"/>
    <file src=".\src\.template.config\template.json" target="content\.template.config\template.json"/>
  </files>
</package>